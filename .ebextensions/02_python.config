option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "aws.settings"
    "PYTHONPATH": "/opt/python/current/app/aws:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: aws/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"
